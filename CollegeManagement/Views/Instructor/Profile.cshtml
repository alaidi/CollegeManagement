@model DataLayer.Entities.Instructor
<div class="row">
    <div class="col-4 ">
        <div class="card" style="width: 18rem;">
            <img src="@Model.Person.PhotoUrl" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">@Model.Person.Name</h5>
                <p class="card-text">
                    @Model.Person.Birthday.ToString("yyyy-MM-dd")
                </p>
            </div>
        </div>
    </div>
    <div class="col-8 ">test</div>
</div>
